{
   "name": "madebyshape",
   "version": "1.0.0",
   "description": "Shape Foundation Framework",
   "main": "gulpfile.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1"
   },
   "author": "MadeByShape (@madebyshape)",
   "authorUrl": "https://madebyshape.co.uk",
   "license": "MIT",
   "urls": {
      "local": "",
      "development": "",
      "staging": "",
      "production": ""
   },
   "base": "./web/",
   "paths": {
      "assets": {
         "base": "web/assets/",
         "sass": "web/assets/scss/",
         "javascript": "web/assets/js/",
         "images": "web/assets/images/",
         "favicons": {
            "source": "web/assets/favicons/",
            "public": "/assets/favicons/"
         },
         "fonts": "web/assets/fonts/",
         "dependencies": "node_modules/"
      },
      "dist": "web/dist/",
      "uploads": "web/uploads/",
      "templates": "templates/"
   },
   "output": {
      "css": "web/dist/app.css",
      "javascript": "web/dist/app.js"
   },
   "files": {
      "sass": "web/assets/scss/**/*.scss",
      "javascript": "web/assets/js/**/*.js",
      "images": "web/assets/images/**/*",
      "favicons": "web/assets/favicons/**/*",
      "fonts": "web/assets/fonts/**/*",
      "templates": "templates/**/*",
      "dist": "web/dist/*",
      "main": {
         "sass": "app.scss",
         "css": "app.css",
         "javascript": "app.js"
      }
   },
   "critical": {
      "target": "",
      "output": "../templates/",
      "urls": [
         {
            "url": "",
            "template": "index"
         }
      ]
   },
   "cssDependencies": [
      "owl.carousel/dist/assets/owl.carousel.css",
      "magnific-popup/dist/magnific-popup.css"
   ],
   "javascriptDependencies": [
      "fg-loadcss/dist/cssrelpreload.js",
      "jquery/dist/jquery.js",
      "platform/platform.js",
      "sticky-kit/dist/sticky-kit.js",
      "magnific-popup/dist/jquery.magnific-popup.js",
      "jquery-lazy/jquery.lazy.js",
      "owl.carousel/dist/owl.carousel.js",
      "object-fit-images/dist/ofi.js"
   ],
   "javascript": [
      "modernizr.js",
      "environment.js"
   ],
   "dependencies": {
      "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
      "browser-sync": "^2.26.3",
      "critical": "^0.9.0",
      "favicons": "^5.3.0",
      "fg-loadcss": "^2.1.0",
      "foundation-sites": "^6.5.1",
      "gulp": "^3.9.1",
      "gulp-autoprefixer": "^3.1.1",
      "gulp-clean-css": "^3.10.0",
      "gulp-concat": "^2.6.1",
      "gulp-htmlmin": "^2.0.0",
      "gulp-imagemin": "^3.3.0",
      "gulp-modernizr": "^1.0.0",
      "gulp-notify": "^3.2.0",
      "gulp-plumber": "^1.2.1",
      "gulp-purgecss": "^1.1.1",
      "gulp-rev": "^7.1.2",
      "gulp-rev-napkin": "^0.1.0",
      "gulp-sass": "^4.0.2",
      "gulp-sass-glob": "^1.0.9",
      "gulp-scsslint": "0.0.5",
      "gulp-sourcemaps": "^1.12.1",
      "gulp-uglify": "^2.1.2",
      "gulp-uncss": "^1.0.6",
      "gulp-watch": "^4.3.9",
      "jquery": "^3.3.1",
      "jquery-lazy": "^1.7.10",
      "magnific-popup": "^1.1.0",
      "object-fit-images": "^3.2.4",
      "owl.carousel": "^2.3.4",
      "platform": "^1.3.5",
      "rev-del": "^1.0.5",
      "run-sequence": "^1.2.2",
      "sticky-kit": "^1.1.3"
   }
}
